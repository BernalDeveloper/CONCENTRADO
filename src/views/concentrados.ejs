</head>
<body>
    <div class="sidebar">
        <div class="logo"></div>
        <ul class="main">
            <li class="active" onclick="showHTML('table-content')">
                <a href="#">
                    <i class="fas fa-list"></i>
                    <span>Rutas</span>
                </a>
            </li>
            <li>
                <a href="#" onclick="showHTML('new-content')">
                    <i class="fas fa-plus"></i>
                    <span>Nuevo</span>
                </a>
            </li>
            <li class="logout">
                <a href="/">
                    <i class="fas fa-sign-out-alt"></i>
                    <span>Log out</span>
                </a>
            </li>
        </ul>
    </div>
    <div class="main-content">
        <div class="header-wrapper">
