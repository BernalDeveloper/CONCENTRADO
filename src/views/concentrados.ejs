                <h2>Concentrado V1.2</h2>
            </div>
            <div class="user-info">
                <div class="search-box">
                    <i class="fa-solid fa-search"></i>
                    <input type="text" placeholder="Search">
                </div>
                <img src="/img/perfil.png" alt="">
            </div>
        </div>
        <%- include('messages-content') %>
        <%- include('table-content') %>
        <%- include('order-table') %>
